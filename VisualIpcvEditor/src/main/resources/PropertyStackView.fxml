<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane ?>
<?import javafx.scene.control.Button?>

<fx:root type="AnchorPane" xmlns:fx="http://javafx.com/fxml/1">
   <VBox spacing="3.0" AnchorPane.topAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.bottomAnchor="0.0">
      <Label fx:id="title" style="-fx-font-weight: bold;" text="Title">
         <VBox.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </VBox.margin>
      </Label>
      <ScrollPane fitToWidth="true">
         <VBox fx:id="propertyList">
            <VBox.margin>
               <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
            </VBox.margin>
         </VBox>
      </ScrollPane>
      <Button fx:id="addButton" text="Add" maxWidth="Infinity">
         <VBox.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
         </VBox.margin>
      </Button>
   </VBox>
</fx:root>
